<StackPanel x:Class="ApEngine.Ctrls.Ctrl_Calibration_AcousticOutput"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ApEngine.Ctrls"
             mc:Ignorable="d"
             d:DesignHeight="450" d:DesignWidth="800">
    <DockPanel>
        <Label Width="108">Connector Type:</Label>
        <!--<ComboBox ItemsSource="{x:Static local:CtrlStatic.OutputConnectorTypes}" SelectedItem="{Binding ConnectorType}" IsEditable="False"></ComboBox>-->
        <Label Content="{Binding ConnectorType}"></Label>
    </DockPanel>
    <DockPanel>
        <Label Width="108">Voltage Ratio:</Label>
        <TextBox Text="{Binding VoltageRatio}"></TextBox>
            <!--<Label>Pa/V</Label>-->
        </DockPanel>
    <DockPanel>
        <Label Width="108">Reference Freq:</Label>
        <TextBox Text="{Binding RefFreq}"></TextBox>
        <!--<Label>kHz</Label>-->
        </DockPanel>
    <!--<DockPanel>
        <Label Width="100">Watts</Label>
        <TextBox Width="80" Text="{Binding Watts}"></TextBox>
        <Label>Unit</Label>
    </DockPanel>-->
</StackPanel>
