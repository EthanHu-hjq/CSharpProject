<?xml version="1.0" encoding="utf-8"?>
<System>
  <Station>
    <BU>Common</BU>
    <CustomerName>Bose</CustomerName>
    <ProjectName>Stan</ProjectName>
    <ProductName>Stan</ProductName>
    <StationName>ARF</StationName>
    <LineNo></LineNo>
    <ProcessNo>ARF</ProcessNo>
    <Stage>EVT</Stage>
    <StationType>System</StationType>
  </Station>
  <Sfcs>
    <SfcsTable>MES_Transfer_Bose_TestStation_Data<!--SFCs Table Name, provide by SFCs--></SfcsTable>
    <EnableSfc>true<!--Enable SFCs or not, Causion: Offline mode only mean no check station and upload into SFCs, the GetPartNo will still work--></EnableSfc>
    <Product>Stan<!--SFCs Product, Use Station.Project as DEFAULT if empty--></Product>
    <Station>Sysem_ARF_Test<!--SFCs Station, Use Station.Station as DEFAULT if empty--></Station>
    <SfcsUploadData>false<!--if upload into SFCs contains detail test data or not--></SfcsUploadData>
    <GetPartNoApi>Get_Model<!--Get_Model_forSub, Get_Model, depends on SFCs team--></GetPartNoApi>
    <Version>1.0<!--SFCs requirements. Default is 1.0--></Version>
  </Sfcs>
  <General>
    <RestrictLimit prefix_defectcode="BESTARR">false<!--If true, the limit will be load from DB not just from sequence file. Default is true--></RestrictLimit>
    <AutoCapitalSn>false<!--Prevent keyboard mistake trigged--></AutoCapitalSn>
    <RE_SerialNumber>^(\w{17}|\w{2})$<!--Regular Expression for SN. In Batch mode. the attr re_subsn is to match the data in MainSN. the attr subsn means how the subsn generated--></RE_SerialNumber>
    <CustomizeInputSn>false<!--If true, the test seq will not handling the SN input, which to take over by Seq itself--></CustomizeInputSn>
    <RE_SocketNumber>^(\d+)#$<!--Regular Expression. Only for Multiple Slot supporting--></RE_SocketNumber>
    <InputUserName>false<!--Reuqire to Input User Name or not--></InputUserName>
    <RunMode>Normal<!--[Obsolete], Model type. Could be Normal or Batch, default: Normal--></RunMode>
    <InputSnMode>Normal<!--InputSnMode type. Could be Normal or Simple, Inherit, PostfixByMainSn, default: Normal--></InputSnMode>
    <SocketCount>2<!--Multi-Slot Jig Support, If configured, the program will create multiple instance to run.--></SocketCount>
    <LoopTest>0<!--[Obsolete]--></LoopTest>
    <ReportPath>D:\TYM_Report\Bose\Stan\ARF<!--Test Report Path. Only impact the report which generated by this system--></ReportPath>
    <Raw_ReportPath>D:\TYM_Raw_Report\Bose\Stan\ARF<!--Test Report Path. To store the raw report which generated by test engine, eg. teststand--></Raw_ReportPath>
    <ErrorHandle>1<!--0: RTEOption_ShowDialog/1: RTEOption_Continue/2: RTEOption_Ignore/3: RTEOption_Abort/4: RTEOption_Retry. For TestStand Only--></ErrorHandle>
    <AutoBreakWhenFailure>false<!--True/False, Default False For TestStand--></AutoBreakWhenFailure>
    <AutoCleanupWhenFailure>true<!--True/False, For TestStand--></AutoCleanupWhenFailure>
    <TestTimeout>-1<!--For Warning the operator if sequence runs over the time. unit second; Default -1--></TestTimeout>
  </General>
</System>