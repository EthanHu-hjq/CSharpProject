<Window x:Class="ToucanCore.UIs.TimeoutMessageBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ToucanCore.UIs"
             mc:Ignorable="d" Width="320" Height="160" WindowStartupLocation="CenterScreen"
             d:DesignHeight="160" d:DesignWidth="320">
    <TextBlock Margin="2" HorizontalAlignment="Center" VerticalAlignment="Center" Text="{Binding Message}"></TextBlock>
</Window>
