<StackPanel x:Class="ApEngine.Ctrls.Ctrl_Calibration_AnalogOutput"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ApEngine.Ctrls"
             mc:Ignorable="d"
             d:DesignHeight="450" d:DesignWidth="800">
    <DockPanel>
        <Label Width="108">Connector Type:</Label>
        <!--<ComboBox ItemsSource="{x:Static local:CtrlStatic.OutputConnectorTypes}" SelectedItem="{Binding ConnectorType}" IsEnabled="False"></ComboBox>-->
        <Label Content="{Binding ConnectorType}"></Label>
    </DockPanel>
    <DockPanel>
        <Label Width="108">dBrG:</Label>
        <TextBox Text="{Binding dBrG}"></TextBox>
        <!--<Label>mVrms</Label>-->
    </DockPanel>
    <DockPanel>
        <Label Width="108">dBm:</Label>
        <TextBox Text="{Binding dBm}"></TextBox>
        <!--<Label>ohm</Label>-->
    </DockPanel>
    <DockPanel>
        <Label Width="108">Watts</Label>
        <TextBox Text="{Binding Watts}"></TextBox>
        <!--<Label>ohm</Label>-->
    </DockPanel>
</StackPanel>
